<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.udea.loaiza.sivan.com.gmail.oandreszr.evaludea">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation= "portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".simulatorActivity"
            android:label="@string/title_activity_simulator"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".learningActivity"
            android:label="@string/title_activity_learning"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".answersActivity"
            android:label="@string/title_activity_answers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".resultActivity"
            android:label="@string/title_activity_result"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
